{
	"editor.fontSize": 17,
	"editor.fontWeight": "normal",
	"editor.fontFamily": "JetBrains Mono, Menlo, Monaco, 'Courier New', monospace",
	"editor.lineHeight": 40,
	"editor.letterSpacing": 1,
	"editor.tabCompletion": "on",
	"editor.wordWrap": "wordWrapColumn",
	"editor.fontLigatures": true,
	"workbench.editor.tabSizing": "shrink",
	"html.format.enable": true,
	"html.format.preserveNewLines": true,
	"files.trimTrailingWhitespace": true,
	"editor.cursorWidth": 3,
	"editor.tabSize": 2,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.endOfLine": "auto",
	"prettier.jsxSingleQuote": true,
	"prettier.useTabs": true,
	"prettier.semi": false,
	"prettier.trailingComma": "none",
	"prettier.singleQuote": true,
	"editor.hover.above": false,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": true,
		"source.sortImports": true
	},
	"editor.renderControlCharacters": true,
	"editor.copyWithSyntaxHighlighting": false,
	"editor.minimap.showSlider": "always",
	"editor.inlineSuggest.enabled": true,
	"editor.suggestSelection": "first",
	"[css]": {
		"editor.suggest.insertMode": "replace",
	},
	"workbench.colorCustomizations": {
		"errorLens.hintForeground": "#ffc600A1",
		"errorLens.hintBackground": "#ffc60003",
		"errorLens.errorBackground": "#ff000005"
	},
	"extensions.ignoreRecommendations": false,
	"files.exclude": {
		"**/.cache": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.next": true,
		"**/.svn": true,
		"**/CVS": true,
		"**/dist": false
	},
	"workbench.tree.indent": 5,
	"editor.mouseWheelZoom": true,
	"emmet.triggerExpansionOnTab": true,
	"editor.minimap.enabled": true,
	"explorer.confirmDragAndDrop": false,
	"editor.parameterHints.enabled": false,
	"git.enabled": true,
	"workbench.startupEditor": "newUntitledFile",
	"better-comments.multilineComments": true,
	"better-comments.highlightPlainText": false,
	"terminal.integrated.fontSize": 18,
	"terminal.integrated.fontWeight": "400",
	"terminal.integrated.fontFamily": "monospace",
	"emmet.syntaxProfiles": {
		"javascript": "jsx",
		"typescript": "tsx"
	},
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"typescript": "typescriptreact"
	},
	"editor.renderWhitespace": "selection", // removes whitespace chars
	"editor.hideCursorInOverviewRuler": true, // removes the folding feature
	"editor.glyphMargin": true, // removes the space used mainly for debugging indicators
	"explorer.openEditors.visible": 1,
	"editor.overviewRulerBorder": true,
	"workbench.editor.showIcons": true, // removes icon from opened files in tabs
	"editor.occurrencesHighlight": true,
	"workbench.iconTheme": "dobri-theme-icons",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"explorer.confirmDelete": false,
	"editor.minimap.size": "fill",
	"editor.cursorBlinking": "expand",
	"editor.cursorStyle": "line",
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"security.workspace.trust.untrustedFiles": "open",
	"cSpell.userWords": [],
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"editor.linkedEditing": true,
	"git.autofetch": true,
	"redhat.telemetry.enabled": true,
	"javascript.format.semicolons": "remove",
	"typescript.format.semicolons": "remove",
	"workbench.editor.wrapTabs": true,
	"workbench.editor.enablePreviewFromQuickOpen": true,
	"workbench.editor.revealIfOpen": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.largeFileOptimizations": true,
	"files.autoSave": "off",
	"vsicons.dontShowNewVersionMessage": true,
	"git.enableSmartCommit": true,
	"github.copilot.enable": {
		"*": true,
		"plaintext": true,
		"markdown": false,
		"scminput": false,
		"yaml": false
	},
	"workbench.editor.untitled.hint": "hidden",
	"editor.accessibilitySupport": "off",
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"editor.stickyTabStops": true,
	"editor.stickyScroll.enabled": true,
	"editor.scrollbar.verticalScrollbarSize": 50,
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"typescript.tsserver.experimental.enableProjectDiagnostics": true,
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"javascript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"typescript.implementationsCodeLens.enabled": true,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"editor.inlayHints.enabled": "offUnlessPressed",
	"window.titleBarStyle": "custom",
	"workbench.colorTheme": "Spirited Away",
	"terminal.integrated.env.windows": {},
	"workbench.productIconTheme": "material-product-icons",
	"chatgpt.lang": "en"
}
